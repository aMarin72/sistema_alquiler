
Table propiedades {
    id int [primary key, increment]
    tipo varchar
    direccion varchar
    precio int
    descripcion varchar
    estado varchar
    created_at datetime
    updated_at datetime
}

Table inquilinos {
    id int [primary key, increment]
    nombres varchar
    email varchar
    telefono varchar
    fecha_nacimiento date
    dni varchar
    created_at datetime
    updated_at datetime
}
